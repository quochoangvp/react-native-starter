{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@apis": "./src/apis",
          "@assets": "./src/assets",
          "@components": "./src/components",
          "@configs": "./src/configs",
          "@languages": "./src/languages",
          "@models": "./src/models",
          "@modules": "./src/modules",
          "@navigators": "./src/navigators",
          "@redux": "./src/redux",
          "@pages": "./src/pages",
          "@utils": "./src/utils"
        }
      }
    ]
  ]
}
